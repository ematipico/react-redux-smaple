{
  "name": "react-redux-smaple",
  "version": "1.0.0",
  "description": "A project to show how redux and react are used",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_PATH=./src cross-env NODE_ENV=test ava --verbose",
    "start": "cross-env NODE_ENV=development browser-sync start -c server.js",
    "deploy": "cross-env NODE_ENV=production webpack --config webpack.config.js && browser-sync start -c deploy.js",
    "lint": "standard --plugins flowtype | snazzy",
    "cover": "cross-env NODE_PATH=./src cross-env NODE_ENV=test nyc ava",
    "report": "nyc report --reporter=html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ematipico/react-redux-smaple.git"
  },
  "keywords": [
    "react",
    "redux",
    "ava",
    "nyc",
    "reddit"
  ],
  "author": "Emanuele Stoppa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ematipico/react-redux-smaple/issues"
  },
  "homepage": "https://github.com/ematipico/react-redux-smaple#readme",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "classnames": "^2.2.5",
    "exports-loader": "^0.6.3",
    "imports-loader": "^0.7.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.3.0",
    "npm": "^4.1.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.0.0-beta.2",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-saga": "^0.14.3",
    "sass-loader": "^4.1.1",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.0",
    "ava": "^0.18.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-tcomb": "^0.3.24",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-plugin-transform-remove-debugger": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.13.0",
    "babel-register": "^6.23.0",
    "browser-sync": "^2.18.6",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "flow-bin": "^0.38.0",
    "flow-cli": "0.0.0-pre",
    "flow-typed": "^2.0.0",
    "jsdom": "^9.11.0",
    "nyc": "^10.1.2",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "sinon": "^1.17.7",
    "snazzy": "^6.0.0",
    "standard": "^8.6.0",
    "standard-loader": "^5.0.0",
    "style-loader": "^0.13.1",
    "tap-difflet": "^0.7.0",
    "tape": "^4.6.3",
    "tcomb": "^3.2.16",
    "uglifyjs-webpack-plugin": "^0.2.1",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "ignore": [
      "app.js"
    ],
    "globals": [
      "Generator",
      "fetch",
      "module"
    ]
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  }
}
