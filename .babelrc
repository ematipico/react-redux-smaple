{
  "env": {
    "test": {
      "presets": ["es2015", "react", "stage-0", "stage-1"],
      "plugins": ["transform-object-assign"]
    },
    "development": {
      "presets": [
        ["es2015", { "modules": false, "loose": true }],
        "react", "stage-0", "stage-1",
      ],
      "plugins": ["transform-object-assign", "react-hot-loader/babel"]
    },
    "production": {
      "presets": [
        ["es2015", { "modules": false, "loose": true }],
        "react", "stage-0", "stage-1",
      ],
      "plugins": ["transform-object-assign"]
    }
  }

}
